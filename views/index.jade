extends layout

block content

	#backgroundGradient
		#imageOverlay

	// Wrapper
	img(id="logo" src="/images/logo.png")
	#wrapper

		// USER LIST
		
		#userList
			table
				thead
					th Rank
					th Name
					th Money
				tbody
		// /USER LIST

		#buttons
			a(href="http://google.com" class="button" id="back") Back
			a(href="http://google.com" class="button" id="next") Next
			

		form(id="search")
			input(type="text" name="steamid" placeholder="Search SteamID")
			button(type="submit" value="Submit") Search
	// /Wrapper

